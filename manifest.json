{
	"manifest_version": 2,
	
 	"name": "github-hide-whitespace-changes",
	"version": "0.1",

	"content_scripts": [
		{
			"matches": [ "http://www.github.com/*", "https://www.github.com/*", "http://github.com/*", "https://github.com/*" ],
			"js": [ "github.js" ],
			"run_at": "document_start"
		}
	]
}